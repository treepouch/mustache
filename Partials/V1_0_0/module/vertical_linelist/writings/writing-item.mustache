{{#terms}}
<ul class="dictionary-list__item" data-item-id="{{itemId}}" data-termIndex="{{termIndex}}">
    <li class="dictionary-list__sound-clip secondary">
        {{#audio}}
        <div class="dictionary-list__sound-icon dictionary-card__speaker icon icon--speaker" data-uri="{{.}}">
        </div>
        {{/audio}}
    </li>
    <li class="dictionary-list__term">
        <div class="dictionary-list__term-container{{#example}} dictionary-list__term-containerWithExample{{/example}}">
            <ul class="dictionary-list__term-inner">
                <li class="dictionary-list__term-title">{{title}}</li>
                <li class="dictionary-list__term-pronunciation">{{#partOfSpeech}}<span class="dictionary-list__pos">{{.}}</span> {{/partOfSpeech}}{{#pronunciation}}{{.}}{{/pronunciation}}</li>
                <li class="dictionary-list__watch-clip">
                    {{#videoLink}}<a class="dictionary-list__watch-clip primary-text-color--{{promoId}}" href="{{.}}">Watch Sketch</a>{{/videoLink}}
                    <div class="dictionary-list__desktop-share js-share" data-url="{{share}}" data-subject="{{subject}}" data-title="{{title}}" data-seriestitle="{{shortTitle}}">
                        <div class="social-icon js-share__button facebook" data-type="facebook" data-graphics="n">
                            <span class="screen-reader-text">Facebook</span>
                        </div>
                        <div class="social-icon js-share__button twitter" data-type="twitter" data-graphics="o">
                            <span class="screen-reader-text">Tweet</span>
                        </div>
                        <div class="social-icon js-share__button email" data-type="email" data-graphics="r">
                            <span class="screen-reader-text">Email</span>
                        </div>
                    </div>
                </li>
                {{#definition}}<li class="dictionary-list__definition">{{.}}</li>{{/definition}}
            </ul>
        </div>
        {{#example}}
        <div class="dictionary-list__example">
            {{#image}}
                <div class="dictionary-list__example-imageWrapper">
                    <img class="dictionary-list__example-image" src="{{.}}">
                </div>
            {{/image}}
            {{^image}}
                {{#usage}}
                    <h4>Usage:</h4>
                    <span class="dictionary-list__example-text">{{.}}</span>
                {{/usage}}
            {{/image}}
        </div>
        {{/example}}
        <div class="dictionary-list__mobile-share js-share" data-url="{{share}}" data-subject="{{subject}}" data-title="{{title}}" data-seriestitle="{{shortTitle}}">
            <div class="social-icon js-share__button facebook" data-type="facebook" data-graphics="n">
                <span class="screen-reader-text">Facebook</span>
            </div>
            <div class="social-icon js-share__button twitter" data-type="twitter" data-graphics="o">
                <span class="screen-reader-text">Tweet</span>
            </div>
            <div class="social-icon js-share__button email" data-type="email" data-graphics="r">
                <span class="screen-reader-text">Email</span>
            </div>
            <div class="social-icon js-share__button whatsapp" data-type="whatsapp" data-social="whatsapp" data-term="{{title}}" data-definition="{{definition}}">
                whatsapp
