{{#data}}
    <div class="ent_m215-module-content">
        <div class="ent_m215-wrap">
            {{#schema}}<script type="application/ld+json">{{{.}}}</script>{{/schema}}
            {{#header}}
                <h1 class="ent_m215-module-header headline">{{header}}</h1>
            {{/header}}

            {{#awards}}
                <div class="ent_m215-award-wrapper">
                    {{#awardTitle}}
                        <h2 class="ent_m215-award-title">{{awardTitle}}</h2>
                    {{/awardTitle}}
                    {{#vote}}
                        <div class="ent_m215-vote-wrapper">
                            <h3 class="ent_m215-vote-invitation">{{nomineesVotingOpen}}</h3>
                            <a class="ent_m215-vote-button primary_btn" href="{{vote}}">{{nomineesVotingButton}}</a>
                        </div>
                    {{/vote}}
                    <ul class="ent_m215-nominee-list">
                        {{#nominees}}
                            <li class="ent_m215-nominee-{{awardLayout}}">
                                <div class="ent_m215-nominee-image-wrap-{{awardLayout}} {{#nomineeVideoInfo}}modal-player-video-item{{/nomineeVideoInfo}}" {{#nomineeVideoInfo}}data-video-id="{{id}}" data-video-title="{{title}}" {{#vevoVideo}}data-is-vevo="vevo" data-vevo-partner-id="{{vevoConfiguration.partnerId}}" data-vevo-site-section="{{vevoConfiguration.siteSection}}"{{/vevoVideo}}{{/nomineeVideoInfo}}>
                                    {{#nomineeUrl}}<a target="_blank" href="{{nomineeUrl}}">{{/nomineeUrl}}
                                    {{> common/image }}
                                    {{#nomineeUrl}}</a>{{/nomineeUrl}}
                                </div>
                                <div class="ent_m215-nominee-meta-wrap-{{awardLayout}} ">
                                    <div class="ent_m215-nominee-meta-{{awardLayout}}">
                                        {{#nomineeText1}}
                                            {{#nomineeUrl}}
                                            <a class="ent_m215-nominee-title-{{awardLayout}}" href="{{.}}" target="_blank">
                                                {{nomineeText1}}
                                            </a>
                                            {{/nomineeUrl}}
                                            {{^nomineeUrl}}
                                            <div class="ent_m215-nominee-title-{{awardLayout}}">
                                                {{nomineeText1}}
                                            </div>
                                            {{/nomineeUrl}}
                                        {{/nomineeText1}}

                                        {{#nomineeText2}}
                                            <div class="ent_m215-nominee-subtitle-{{awardLayout}}">
                                                {{nomineeText2}}
                                            </div>
                                        {{/nomineeText2}}

                                        {{#nomineeText3}}
                                            <div class="ent_m215-nominee-subtitle-{{awardLayout}}">
                                                {{nomineeText3}}
                                            </div>
                                        {{/nomineeText3}}
                                    </div>

                                </div>
                            </li>
                        {{/nominees}}
                    </ul>
                </div>
            {{/awards}}
        </div>
    </div>
{{/data}}
