{{#data}}
    <section id="section-azshows">
        <div class="container">
            <div class="row">
                <div class="alphafilter-wrap">

                    {{#parameters}}
                        {{#displayBoxAd}}
                            <div id="az-ad">
                                <div class="ad-container" data-adsize="300x250" data-ad-sizes="300x250"></div>
                            </div>
                        {{/displayBoxAd}}
                    {{/parameters}}

                    <div class="col-lg-12">
                        <header class="section-header-1 section-header-lg">
                            {{#headerText}}
                                <h2>{{.}}</h2>
                            {{/headerText}}
                        </header>
                    </div>

                    <div class="col-lg-12">
                        <div class="accordion" id="alphafilter">
                            <div class="accordion-group group panel">

                                {{#items}}

                                    <div class="accordion-heading {{#showByDefault}}active{{/showByDefault}}" id="label{{selector}}">
                                        <span class="accordion-toggle {{^sortedItems}}disabled{{/sortedItems}}" data-parent="#alphafilter" data-char="{{selector}}">
                                            {{letter}}
                                            <span class="hidden-lg icon-chevron-down pull-right"></span>
                                        </span>
                                    </div>

                                    <div class="accordion-body" id="collapse{{selector}}" data-char="{{selector}}" style="display: {{#showByDefault}}block{{/showByDefault}}{{^showByDefault}}none{{/showByDefault}};">
                                        <div class="accordion-inner">
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <ul>
                                                        {{#sortedItems}}
                                                            <li class="show-item">
                                                                <a href="{{.url}}">
                                                                    {{.title}}
                                                                </a>
                                                            </li>
                                                        {{/sortedItems}}
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                {{/items}}

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
{{/data}}
