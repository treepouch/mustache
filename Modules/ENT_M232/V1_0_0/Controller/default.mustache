{{#data}}
    <div class="ent_m232-module-wrapper">
        {{#moduleTitle}}
            <h2 class="ent_m232-module-header">{{moduleTitle}}</h2>
        {{/moduleTitle}}

        <div class="ent_m232-content-wrap">
            <div class="ent_m232-summary-wrap">
                {{#eventWeekday}}
                    <div class="ent_m232-summary-weekday">
                        {{eventWeekday}}
                    </div>
                {{/eventWeekday}}
                {{#eventDate}}
                    <div class="ent_m232-summary-date">
                        {{eventDate}}
                    </div>
                {{/eventDate}}
                {{#eventTimes}}
                    <div class="ent_m232-summary-times">
                        {{{eventTimes}}}
                    </div>
                {{/eventTimes}}
                {{#eventTimezone}}
                    <div class="ent_m232-summary-timezone">
